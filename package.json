{
  "name": "microservices-nodejs-handlebars-frontend",
  "version": "1.0.0",
  "description": "A Front-end service with node.js and handlebars for microservice example",
  "main": "server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "nodemon ./src/server.js",
