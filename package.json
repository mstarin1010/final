    "handlebars-helpers": "^0.10.0",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "bower": "^1.8.4",
    "mocha": "^5.2.0",
    "nock": "^9.0.14",
    "nodemon": "^1.12.0"
  }
